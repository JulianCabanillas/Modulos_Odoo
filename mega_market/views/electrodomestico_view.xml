<odoo>
  <data>

    <record model="ir.ui.view" id="view_electrodomestico_tree">
      <field name="name">Electrodomestico List</field>
      <field name="model">electrodomestico</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="codigo"/>
          <field name="precio_venta"/>
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="view_electrodomestico_form">
      <field name="name">Electrodomestico Form</field>
      <field name="model">electrodomestico</field>
      <field name="arch" type="xml">
        <form string="Electrodoméstico">
          <group>
            <field name="name"/>
            <field name="codigo"/>
            <field name="pais_id"/>
            <field name="importe_compra"/>
            <field name="moneda_id"/>
            <field name="precio_venta"/>
          </group>
        </form>
      </field>
    </record>

    <record model="ir.actions.act_window" id="action_electrodomestico">
      <field name="name">Electrodomésticos</field>
      <field name="res_model">electrodomestico</field>
      <field name="view_mode">tree,form</field>
    </record>

  </data>
</odoo>
